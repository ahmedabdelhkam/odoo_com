<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record model="ir.module.category" id="module_category_realestate">
            <field name="name">Realestate Management</field>
            <field name="sequence">70</field>
            <field name="description">Helps you manage the project.</field>
        </record>
        <record model="res.groups" id="group_realestate_engineer">
            <field name="category_id" ref="module_category_realestate"/>
            <field name="name">Engineer</field>
        </record>
        <record model="res.groups" id="group_realestate_manager">
            <field name="category_id" ref="module_category_realestate"/>
            <field name="name">Manager</field>
            <field name="implied_ids" eval="[(4, ref('group_realestate_engineer'))]"/>
        </record>
        <record id="realestate_record_rule" model="ir.rule">
            <field name="name">just engineers</field>
            <field name="model_id" ref="model_realestate_formation"/>
            <field name="domain_force">[('engineer_id.user_id','=',user.id)]</field>
            <!--            <field name="domain_force">[]</field>-->
            <field name="groups" eval="[(4, ref('group_realestate_engineer'))]"/>
        </record>
         <record id="realestate_record_rule_manager" model="ir.rule">
            <field name="name">just manager</field>
            <field name="model_id" ref="model_realestate_formation"/>
            <field name="domain_force">[]</field>
            <field name="groups" eval="[(4, ref('group_realestate_manager'))]"/>
        </record>

    </data>
</odoo>